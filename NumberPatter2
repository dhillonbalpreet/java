/*
1234
123
12
1
*/

class NumberPatter2 {
    public static void main(String[] args) {
        
        int i=0,j=0;
        for(i=4;i>=0;i--)
        {
          for(j=1;j<=i;j++)
            {
            System.out.print(j);
            
            }  
           System.out.println(); 
        }
        
    }
}
