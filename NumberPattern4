/*
1
01
101
0101
10101
*/
class NumberPattern4 {
    public static void main(String[] args) {
        
        int i=0,j=0,c=1;
        for(i=0;i<=5;i++)
        {
          for(j=1;j<=i;j++)
            {
                if((i+j)%2==0)
                {
            System.out.print("1");
                }
                else
            {
            System.out.print("0");
                }
            }  
           System.out.println(); 
        }
        
    }
}
