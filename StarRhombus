/*
     *****
    *****
   *****
  *****
 *****
*/
class StarRhombus {
    public static void main(String[] args) {
        int i = 0, j=0,n=4;
        for(i=n; i>=0;i--)
            {
                for(j=0;j<=i;j++)
                { 
                        System.out.print(" ");
                }
                for(j=0;j<=n;j++)
                {    
                    System.out.print("*");
                }System.out.println();
                }
            }
}
