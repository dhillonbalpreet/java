class TriagleFromRight
{
  public static void main(String args[])
  {
    int c= 5, r = 4,i=0,j=0;
      for(i=4 ; i>=0;i--)
      {
        for(j=0;j<=4;j++)
          {
            if(i==j || j>i)
            {
             System.out.print("*"); 
            }
            else
            {System.out.print(" ");}
            
          
          }
        System.out.println();
      }
  }
}
